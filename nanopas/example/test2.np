procedure main

arr2        :   array [10] of int;
count,a,b   :   int;

begin
    if count<0 then
        count:=1 
    else
        if a>b then
        begin
            a:=arr2[count mod 10]/b;
            while a<10 do
            begin
                a:=a+arr2[count];
                count:=count+1
            end;
        end
end
